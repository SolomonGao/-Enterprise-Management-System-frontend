{
    "version": 2,
    "builds": [
        {
          "src": "next.config.js",
          "use": "@vercel/next"
        }
      ],
    "functions": {
        "api/*.ts": {
          "runtime": "nodejs16.x"
        }
      },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
  "outputDirectory": "out",
  "scripts": {
    "build": "next build && next export"
  }
}